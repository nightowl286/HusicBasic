<UserControl x:Class="HusicBasic.Views.Dialogs.AddSong.AddSongDialog"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:prism="http://prismlibrary.com/"          
             xmlns:views="clr-namespace:HusicBasic.Views.Dialogs.AddSong"
             xmlns:vms="clr-namespace:HusicBasic.ViewModels.Dialogs.AddSong"
             xmlns:i="http://schemas.microsoft.com/xaml/behaviors"
             prism:ViewModelLocator.AutoWireViewModel="True" Focusable="False">
    <i:Interaction.Triggers>
        <i:EventTrigger EventName="Loaded">
            <prism:InvokeCommandAction Command="{Binding LoadedCommand}"/>
        </i:EventTrigger>
    </i:Interaction.Triggers>
    <ContentControl x:Name="mainRegion" Focusable="False" prism:RegionManager.RegionName="AddSongMainRegion"/>
</UserControl>
